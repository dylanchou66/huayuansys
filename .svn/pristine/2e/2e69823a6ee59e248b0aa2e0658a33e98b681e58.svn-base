        <div class="col-xs-12 col-sm-8">
            <input id="c-type_id" data-rule="required" data-source="dict_v/index" data-params='{"custom[status]":"1","custom[dict_id]":"26"}' class="form-control selectpage" name="row[type_id]" type="text" value="">
