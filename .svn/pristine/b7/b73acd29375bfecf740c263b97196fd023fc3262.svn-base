        <div class="col-xs-12 col-sm-8">
            <input id="c-check_ids" data-rule="required" data-source="dict_v/index" data-params='{"custom[status]":"1","custom[dict_id]":"22"}' data-multiple="true" class="form-control selectpage" name="row[check_ids]" type="text" value="{$row.check_ids|htmlentities}">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Iso')}:</label>
        <div class="col-xs-12 col-sm-8">
                        
            <select  id="c-iso" data-rule="required" class="form-control selectpicker" name="row[iso]">
                {foreach name="isoList" item="vo"}
                    <option value="{$key}" {in name="key" value="$row.iso"}selected{/in}>{$vo}</option>
                {/foreach}
            </select>

        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Copy')}:</label>
        <div class="col-xs-12 col-sm-8">
                        
            <select  id="c-copy" data-rule="required" class="form-control selectpicker" name="row[copy]">
                {foreach name="copyList" item="vo"}
                    <option value="{$key}" {in name="key" value="$row.copy"}selected{/in}>{$vo}</option>
                {/foreach}
            </select>

        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Checkimages')}:</label>
        <div class="col-xs-12 col-sm-8">
            <div class="input-group">
                <input id="c-checkimages" data-rule="required" class="form-control" size="50" name="row[checkimages]" type="text" value="{$row.checkimages|htmlentities}">
                <div class="input-group-addon no-border no-padding">
                    <span><button type="button" id="faupload-checkimages" class="btn btn-danger faupload" data-input-id="c-checkimages" data-mimetype="image/gif,image/jpeg,image/png,image/jpg,image/bmp,application/pdf" data-multiple="true" data-preview-id="p-checkimages"><i class="fa fa-upload"></i> {:__('Upload')}</button></span>
                    <span><button type="button" id="fachoose-checkimages" class="btn btn-primary fachoose" data-input-id="c-checkimages" data-mimetype="image/*" data-multiple="true"><i class="fa fa-list"></i> {:__('Choose')}</button></span>
                </div>
                <span class="msg-box n-right" for="c-checkimages"></span>
            </div>
            <ul class="row list-inline faupload-preview" id="p-checkimages"></ul>
        </div>
    </div>
<!--    <div class="form-group">-->
<!--        <label class="control-label col-xs-12 col-sm-2">{:__('Weigh')}:</label>-->
