            <input id="c-operations" class="form-control" name="row[operations]" type="text" value="{$row.operations|htmlentities}">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Operphone')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-operphone" class="form-control" name="row[operphone]" type="text" value="{$row.operphone|htmlentities}">
        </div>
