<form id="edit-form" class="form-horizontal" role="form" data-toggle="validator" method="POST" action="">

    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Company')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-company" data-rule="required" class="form-control" name="row[company]" type="text" value="{$row.company|htmlentities}">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Dict_values_id')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-dict_values_id" data-rule="required" data-source="dict_v/index" class="form-control selectpage" name="row[dict_values_id]" data-params='{"custom[status]":"1","custom[dict_id]":"19"}' type="text" value="{$row.dict_values_id|htmlentities}">
        </div>
    </div>
