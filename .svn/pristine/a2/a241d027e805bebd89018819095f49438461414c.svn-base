<form id="edit-form" class="form-horizontal" role="form" data-toggle="validator" method="POST" action="">

    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Shenqin_type_id')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-shenqin_type_id" data-rule="required" data-source="dict_v/index" data-params='{"custom[status]":"1","custom[dict_id]":"38"}' class="form-control selectpage" name="row[shenqin_type_id]" type="text" value="{$row.shenqin_type_id|htmlentities}">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('S_time')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-s_time" class="form-control datetimepicker" data-date-format="YYYY-MM-DD HH:mm:ss" data-use-current="true" name="row[s_time]" type="text" value="{$row.s_time}">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('E_time')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-e_time" class="form-control datetimepicker" data-date-format="YYYY-MM-DD HH:mm:ss" data-use-current="true" name="row[e_time]" type="text" value="{$row.e_time}">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('From_city_id')}:</label>
        <div class="col-xs-12 col-sm-8">
<!--            <input id="c-from_s_id" data-rule="required" data-source="from/s/index" class="form-control selectpage" name="row[from_s_id]" type="text" value="{$row.from_s_id|htmlentities}">-->
            <div class="form-inline" data-toggle="cxselect" data-selects="province,city,area">
                <select class="province form-control" name="row[from_s_id]" data-url="ajax/area" >
                    <option value="{$row.from_s_id|htmlentities}" ></option>
                </select>
                <select class="city form-control" name="row[from_city_id]" data-query-name="province" data-url="ajax/area" >
                    <option value="{$row.from_city_id|htmlentities}" ></option>
                </select>
            </div>
        </div>
    </div>
<!--    <div class="form-group">-->
<!--        <label class="control-label col-xs-12 col-sm-2">{:__('From_city_id')}:</label>-->
<!--        <div class="col-xs-12 col-sm-8">-->
<!--            <input id="c-from_city_id" data-rule="required" data-source="from/city/index" class="form-control selectpage" name="row[from_city_id]" type="text" value="{$row.from_city_id|htmlentities}">-->
<!--        </div>-->
<!--    </div>-->
<!--    <div class="form-group">-->
<!--        <label class="control-label col-xs-12 col-sm-2">{:__('To_s_id')}:</label>-->
<!--        <div class="col-xs-12 col-sm-8">-->
<!--            <input id="c-to_s_id" data-rule="required" data-source="to/s/index" class="form-control selectpage" name="row[to_s_id]" type="text" value="{$row.to_s_id|htmlentities}">-->
<!--        </div>-->
<!--    </div>-->
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('To_city_id')}:</label>
        <div class="col-xs-12 col-sm-8">
<!--            <input id="c-to_city_id" data-rule="required" data-source="to/city/index" class="form-control selectpage" name="row[to_city_id]" type="text" value="{$row.to_city_id|htmlentities}">-->
            <div class="form-inline" data-toggle="cxselect" data-selects="province,city,area">
                <select class="province form-control" name="row[to_s_id]" data-url="ajax/area" >
                    <option value="{$row.to_s_id|htmlentities}" ></option>
                </select>
                <select class="city form-control" name="row[to_city_id]" data-query-name="province" data-url="ajax/area" >
                    <option value="{$row.to_city_id|htmlentities}" ></option>
                </select>
            </div>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Gongju_id')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-gongju_id" data-rule="required" data-source="dict_v/index" data-params='{"custom[status]":"1","custom[dict_id]":"39"}' class="form-control selectpage" name="row[gongju_id]" type="text" value="{$row.gongju_id|htmlentities}">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Ps')}:</label>
        <div class="col-xs-12 col-sm-8">
            <textarea id="c-ps" class="form-control " rows="5" name="row[ps]" cols="50">{$row.ps|htmlentities}</textarea>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Money')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-money" class="form-control" name="row[money]" type="text" value="{$row.money|htmlentities}">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-12 col-sm-2">{:__('Adduser')}:</label>
        <div class="col-xs-12 col-sm-8">
<!--            <input id="c-adduser" class="form-control" name="row[adduser]" type="number" value="{$row.adduser|htmlentities}">-->
            <input id="c-adduser" data-rule="required" data-source="auth/admin" class="form-control selectpage" name="row[adduser]" data-field="nickname" type="text" value="{$row.adduser|htmlentities}" disabled>

        </div>
    </div>
<!--    <div class="form-group">-->
<!--        <label class="control-label col-xs-12 col-sm-2">{:__('Status')}:</label>-->
<!--        <div class="col-xs-12 col-sm-8">-->
<!--            -->
<!--            <div class="radio">-->
<!--            {foreach name="statusList" item="vo"}-->
<!--            <label for="row[status]-{$key}"><input id="row[status]-{$key}" name="row[status]" type="radio" value="{$key}" {in name="key" value="$row.status"}checked{/in} /> {$vo}</label> -->
<!--            {/foreach}-->
<!--            </div>-->

<!--        </div>-->
<!--    </div>-->
    <div class="form-group layer-footer">
        <label class="control-label col-xs-12 col-sm-2"></label>
        <div class="col-xs-12 col-sm-8">
            <button type="submit" class="btn btn-success btn-embossed disabled">{:__('OK')}</button>
            <button type="reset" class="btn btn-default btn-embossed">{:__('Reset')}</button>
        </div>
    </div>
</form>
